<p id="notice"><%= notice %></p>

<h1>Yonc√©gram</h1>
<br>
<%= link_to 'ADD NEW PHOTO', new_photo_path %>
<br>
<br>
  <tbody>

  <% @photos.each do |photo| %>
      <br>
      <%= photo.title.upcase %>
      <p><%= image_tag photo.image, width: '300' %> </p>
      <p><em> posted by <strong><%= photo.user.email %> </strong></em></p>
        <td><%= link_to 'Show', photo %></td>
      <% if photo.user.email == current_user.email %>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <br>
        <br>
      <% end %>
      <hr>
  <% end %>



